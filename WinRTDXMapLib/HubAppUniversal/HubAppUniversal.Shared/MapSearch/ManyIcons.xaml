<Page
    x:Class="HubAppUniversal.MapSearch.ManyIcons"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid >

        <Grid.RowDefinitions>
            <RowDefinition Height="auto"></RowDefinition>
            <RowDefinition Height="5*"></RowDefinition>
        </Grid.RowDefinitions>

        <Grid Grid.Row="0">
            <StackPanel>
                <StackPanel Orientation="Vertical" HorizontalAlignment="Center"   >
                    <Button Content="已有层加点" Click="ExistedLayerAdd"></Button>
                    <Button Content="先加点再加层" Click="AddLayerContainIcons"></Button>
                    <Button Content="显示APPID" Click="GetAppid"></Button>

                </StackPanel>
            </StackPanel>
        </Grid>



        <Grid Grid.Row="1" x:Name="ContentGrid"></Grid>


    </Grid>
</Page>
